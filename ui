-- Timer Rows Function
function createTimerRow(parent, name, label, layoutOrder)
    local row = Instance.new('Frame')
    local rowLabel = Instance.new('TextLabel')
    local timer = Instance.new('TextLabel')
    local tpBtn = Instance.new('TextButton')
    local corner = Instance.new('UICorner')
    local btnCorner = Instance.new('UICorner')
    
    row.Name = name .. 'Row'
    row.Parent = parent
    row.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
    row.BorderSizePixel = 0
    row.Size = UDim2.new(1, -16, 0, 45)
    row.LayoutOrder = layoutOrder
    
    corner.CornerRadius = UDim.new(0, 6)
    corner.Parent = row
    
    rowLabel.Name = name .. 'Label'
    rowLabel.Parent = row
    rowLabel.BackgroundTransparency = 1
    rowLabel.Position = UDim2.new(0, 12, 0, 0)
    rowLabel.Size = UDim2.new(0, 100, 1, 0)
    rowLabel.Font = Enum.Font.Gotham
    rowLabel.Text = label
    rowLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    rowLabel.TextSize = 13
    rowLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    timer.Name = name .. 'Timer'
    timer.Parent = row
    timer.BackgroundTransparency = 1
    timer.Position = UDim2.new(0, 120, 0, 0)
    timer.Size = UDim2.new(0, 100, 1, 0)
    timer.Font = Enum.Font.GothamBold
    timer.Text = '00:00'
    timer.TextColor3 = Color3.fromRGB(100, 200, 255)
    timer.TextSize = 13
    timer.TextXAlignment = Enum.TextXAlignment.Left
    
    tpBtn.Name = name .. 'Tp'
    tpBtn.Parent = row
    tpBtn.BackgroundColor3 = Color3.fromRGB(76, 175, 80)
    tpBtn.BorderSizePixel = 0
    tpBtn.Position = UDim2.new(1, -50, 0, 8)
    tpBtn.Size = UDim2.new(0, 35, 0, 28)
    tpBtn.Font = Enum.Font.GothamBold
    tpBtn.Text = 'TP'
    tpBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    tpBtn.TextSize = 12
    
    btnCorner.CornerRadius = UDim.new(0, 4)
    btnCorner.Parent = tpBtn
    
    return row, timer, tpBtn
end

local jungleRow, jungleTimer, jungleTp = createTimerRow(TimersFrame, 'Jungle', 'Jungle', 1)
local rockyRow, rockyTimer, rockyTp = createTimerRow(TimersFrame, 'Rocky', 'Rocky Rampage', 2)
local neoRow, neoTimer, neoTp = createTimerRow(TimersFrame, 'Neo', 'Neo Nexus', 3)
local witheredRow, witheredTimer, witheredTp = createTimerRow(TimersFrame, 'Withered', 'Withered Waters', 4)

