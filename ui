-- Options Function
function createOption(parent, name, labelText, layoutOrder, hasInput)
    local option = Instance.new('Frame')
    local toggle = Instance.new('TextButton')
    local corner = Instance.new('UICorner')
    local toggleCorner = Instance.new('UICorner')
    local label = Instance.new('TextLabel')
    local input
    
    option.Name = name .. 'Option'
    option.Parent = parent
    option.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
    option.BorderSizePixel = 0
    option.Size = UDim2.new(1, -16, 0, 50)
    option.LayoutOrder = layoutOrder
    
    corner.CornerRadius = UDim.new(0, 6)
    corner.Parent = option
    
    toggle.Name = name .. 'Toggle'
    toggle.Parent = option
    toggle.BackgroundColor3 = Color3.fromRGB(70, 70, 75)
    toggle.BorderSizePixel = 0
    toggle.Position = UDim2.new(0, 12, 0, 13)
    toggle.Size = UDim2.new(0, 24, 0, 24)
    toggle.Font = Enum.Font.GothamBold
    toggle.Text = ''
    toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggle.TextSize = 12
    
    toggleCorner.CornerRadius = UDim.new(0, 4)
    toggleCorner.Parent = toggle
    
    label.Name = name .. 'Label'
    label.Parent = option
    label.BackgroundTransparency = 1
    label.Position = UDim2.new(0, 45, 0, 0)
    label.Size = UDim2.new(0, hasInput and 150 or 200, 1, 0)
    label.Font = Enum.Font.Gotham
    label.Text = labelText
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
    label.TextSize = 13
    label.TextXAlignment = Enum.TextXAlignment.Left
    
    if hasInput then
        input = Instance.new('TextBox')
        local inputCorner = Instance.new('UICorner')
        
        input.Name = name .. 'Input'
        input.Parent = option
        input.BackgroundColor3 = Color3.fromRGB(50, 50, 55)
        input.BorderSizePixel = 0
        input.Position = UDim2.new(1, -80, 0, 13)
        input.Size = UDim2.new(0, 60, 0, 24)
        input.Font = Enum.Font.Gotham
        input.PlaceholderText = '3.5'
        input.Text = '3.5'
        input.TextColor3 = Color3.fromRGB(255, 255, 255)
        input.TextSize = 12
        input.TextXAlignment = Enum.TextXAlignment.Center
        
        inputCorner.CornerRadius = UDim.new(0, 4)
        inputCorner.Parent = input
    end
    
    return option, toggle, input
end

local highlightOption, highlightToggle = createOption(OptionsContainer, 'Highlight', 'Highlight Lucky Blocks', 1, false)
local reachOption, reachToggle, reachInput = createOption(OptionsContainer, 'Reach', 'Enable Reach', 2, true)
local infYieldOption, infYieldToggle = createOption(OptionsContainer, 'InfYield', 'Load Infinite Yield', 3, false)

